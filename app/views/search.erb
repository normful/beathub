<div class="container">
  <div class="row">
    <div class="col-lg-8 col-sm-8">
      <h2 class="heading">Search Results</h2>
        <% if @livesets.empty? %>
          <p>No results found. :( </p>
        <% else %>
          <% @livesets.each do |liveset| %>
          <p>
            <a href="liveset/<%= liveset.id %>">
            <%= liveset.artist %> - <%= liveset.title %> - <%= liveset.date_aired %></a>
          </p>
          <% end %>
        <% end %>
      </p>
    </div>
  </div>
</div>
