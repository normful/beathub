<div id="myCarousel" class="carousel slide" data-ride="carousel">
  <ol class="carousel-indicators">
    <li data-target="#myCarousel" data-slide-to="0" class=""></li>
    <li data-target="#myCarousel" data-slide-to="1" class=""></li>
    <li data-target="#myCarousel" data-slide-to="2" class="active"></li>
  </ol>
  <div class="carousel-inner">
    <div class="item active">
      <img src="../assets/purple-rave.jpg"/>
      <div class="container">
        <div class="carousel-caption">
          <h1>Your source for the latest mixes from the world's hottest DJs</h1>
        </div>
      </div>
    </div>
    <div class="item">
      <img src="../assets/armin.jpg"/>
      <div class="container">
        <div class="carousel-caption">
          <h1>Download full mixes, cuesheets, split mixes or individual tracks</h1>
        </div>
      </div>
    </div>
    <div class="item">
      <img src="../assets/blue-rave.jpg"/>
      <div class="container">
        <div class="carousel-caption">
          <h1>Listen to the newest unreleased tracks split from the latest mixes</h1>
        </div>
      </div>
    </div>
  </div>
  <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev"><span class="glyphicon glyphicon-chevron-left"></span></a>
  <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next"><span class="glyphicon glyphicon-chevron-right"></span></a>
</div>
<div class="container">
  <div class="row">
    <div class="col-lg-8 col-sm-8">
      <h2>Latest Sets</h2>
      <% @livesets.each do |liveset| %>
        <p>
          <a href="liveset/<%= liveset.id %>">
          <%= liveset.artist %> - <%= liveset.title %> - <%= liveset.date_aired %></a>
        </p>
      <% end %>
      <%= will_paginate @livesets %>
    </div>
  </div>
</div>
